<?xml version="1.0"?>
<!-- Fetch Mobile Manipulator Robot - Simplified URDF for sim3d -->
<!-- Based on Fetch Robotics specifications -->
<robot name="fetch">

  <!-- Base Link (Mobile Base) -->
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder radius="0.29" length="0.32"/>
      </geometry>
      <origin xyz="0 0 0.16" rpy="0 0 0"/>
      <material name="fetch_gray">
        <color rgba="0.3 0.3 0.3 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.29" length="0.32"/>
      </geometry>
      <origin xyz="0 0 0.16" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="70.0"/>
      <origin xyz="0 0 0.16"/>
      <inertia ixx="2.5" ixy="0" ixz="0" iyy="2.5" iyz="0" izz="3.0"/>
    </inertial>
  </link>

  <!-- Left Wheel -->
  <link name="l_wheel_link">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.04"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.04"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.004"/>
    </inertial>
  </link>

  <joint name="l_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="l_wheel_link"/>
    <origin xyz="0.0 0.22 0.06" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.5" friction="1.0"/>
  </joint>

  <!-- Right Wheel -->
  <link name="r_wheel_link">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.04"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.04"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.004"/>
    </inertial>
  </link>

  <joint name="r_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="r_wheel_link"/>
    <origin xyz="0.0 -0.22 0.06" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.5" friction="1.0"/>
  </joint>

  <!-- Torso (Vertical Column) -->
  <link name="torso_link">
    <visual>
      <geometry>
        <box size="0.1 0.15 0.6"/>
      </geometry>
      <origin xyz="0 0 0.3" rpy="0 0 0"/>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.15 0.6"/>
      </geometry>
      <origin xyz="0 0 0.3" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="10.0"/>
      <origin xyz="0 0 0.3"/>
      <inertia ixx="0.5" ixy="0" ixz="0" iyy="0.5" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <joint name="torso_lift_joint" type="prismatic">
    <parent link="base_link"/>
    <child link="torso_link"/>
    <origin xyz="-0.08 0 0.32" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0.0" upper="0.4" effort="500" velocity="0.1"/>
    <dynamics damping="100"/>
  </joint>

  <!-- Shoulder Pan -->
  <link name="shoulder_pan_link">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.12"/>
      </geometry>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.12"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.5"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <joint name="shoulder_pan_joint" type="revolute">
    <parent link="torso_link"/>
    <child link="shoulder_pan_link"/>
    <origin xyz="0.12 0 0.55" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.6" upper="1.6" effort="33" velocity="1.57"/>
    <dynamics damping="1.0"/>
  </joint>

  <!-- Shoulder Lift -->
  <link name="shoulder_lift_link">
    <visual>
      <geometry>
        <box size="0.08 0.12 0.08"/>
      </geometry>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.08 0.12 0.08"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.008" ixy="0" ixz="0" iyy="0.008" iyz="0" izz="0.008"/>
    </inertial>
  </link>

  <joint name="shoulder_lift_joint" type="revolute">
    <parent link="shoulder_pan_link"/>
    <child link="shoulder_lift_link"/>
    <origin xyz="0.12 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.22" upper="1.52" effort="131" velocity="1.57"/>
    <dynamics damping="1.0"/>
  </joint>

  <!-- Upper Arm -->
  <link name="upperarm_link">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.25"/>
      </geometry>
      <origin xyz="0.125 0 0" rpy="0 1.5708 0"/>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.04" length="0.25"/>
      </geometry>
      <origin xyz="0.125 0 0" rpy="0 1.5708 0"/>
    </collision>
    <inertial>
      <mass value="2.5"/>
      <origin xyz="0.125 0 0"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.005"/>
    </inertial>
  </link>

  <joint name="upperarm_roll_joint" type="revolute">
    <parent link="shoulder_lift_link"/>
    <child link="upperarm_link"/>
    <origin xyz="0.0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="76" velocity="1.57"/>
    <dynamics damping="1.0"/>
  </joint>

  <!-- Elbow -->
  <link name="elbow_link">
    <visual>
      <geometry>
        <box size="0.06 0.08 0.06"/>
      </geometry>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.06 0.08 0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.5"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/>
    </inertial>
  </link>

  <joint name="elbow_flex_joint" type="revolute">
    <parent link="upperarm_link"/>
    <child link="elbow_link"/>
    <origin xyz="0.25 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.25" upper="2.25" effort="66" velocity="1.57"/>
    <dynamics damping="1.0"/>
  </joint>

  <!-- Forearm -->
  <link name="forearm_link">
    <visual>
      <geometry>
        <cylinder radius="0.035" length="0.2"/>
      </geometry>
      <origin xyz="0.1 0 0" rpy="0 1.5708 0"/>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.035" length="0.2"/>
      </geometry>
      <origin xyz="0.1 0 0" rpy="0 1.5708 0"/>
    </collision>
    <inertial>
      <mass value="1.5"/>
      <origin xyz="0.1 0 0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.003"/>
    </inertial>
  </link>

  <joint name="forearm_roll_joint" type="revolute">
    <parent link="elbow_link"/>
    <child link="forearm_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="18" velocity="1.57"/>
    <dynamics damping="1.0"/>
  </joint>

  <!-- Wrist Flex -->
  <link name="wrist_flex_link">
    <visual>
      <geometry>
        <box size="0.05 0.06 0.05"/>
      </geometry>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.06 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.8"/>
      <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
    </inertial>
  </link>

  <joint name="wrist_flex_joint" type="revolute">
    <parent link="forearm_link"/>
    <child link="wrist_flex_link"/>
    <origin xyz="0.2 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.16" upper="2.16" effort="18" velocity="2.26"/>
    <dynamics damping="0.5"/>
  </joint>

  <!-- Wrist Roll -->
  <link name="wrist_roll_link">
    <visual>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
    </collision>
    <inertial>
      <mass value="0.4"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="wrist_roll_joint" type="revolute">
    <parent link="wrist_flex_link"/>
    <child link="wrist_roll_link"/>
    <origin xyz="0.05 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="18" velocity="2.26"/>
    <dynamics damping="0.5"/>
  </joint>

  <!-- Gripper -->
  <link name="gripper_link">
    <visual>
      <geometry>
        <box size="0.08 0.1 0.04"/>
      </geometry>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.08 0.1 0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="gripper_joint" type="fixed">
    <parent link="wrist_roll_link"/>
    <child link="gripper_link"/>
    <origin xyz="0.06 0 0" rpy="0 0 0"/>
  </joint>

  <!-- Left Finger -->
  <link name="l_gripper_finger_link">
    <visual>
      <geometry>
        <box size="0.06 0.01 0.03"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.06 0.01 0.03"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="l_gripper_finger_joint" type="prismatic">
    <parent link="gripper_link"/>
    <child link="l_gripper_finger_link"/>
    <origin xyz="0.04 0.03 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0.0" upper="0.05" effort="60" velocity="0.05"/>
  </joint>

  <!-- Right Finger -->
  <link name="r_gripper_finger_link">
    <visual>
      <geometry>
        <box size="0.06 0.01 0.03"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.06 0.01 0.03"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="r_gripper_finger_joint" type="prismatic">
    <parent link="gripper_link"/>
    <child link="r_gripper_finger_link"/>
    <origin xyz="0.04 -0.03 0" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit lower="0.0" upper="0.05" effort="60" velocity="0.05"/>
    <mimic joint="l_gripper_finger_joint"/>
  </joint>

  <!-- Head Pan -->
  <link name="head_pan_link">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.06"/>
      </geometry>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="head_pan_joint" type="revolute">
    <parent link="torso_link"/>
    <child link="head_pan_link"/>
    <origin xyz="0.05 0 0.65" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.57"/>
    <dynamics damping="0.5"/>
  </joint>

  <!-- Head Tilt -->
  <link name="head_tilt_link">
    <visual>
      <geometry>
        <box size="0.15 0.1 0.08"/>
      </geometry>
      <material name="fetch_gray"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.15 0.1 0.08"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
    </inertial>
  </link>

  <joint name="head_tilt_joint" type="revolute">
    <parent link="head_pan_link"/>
    <child link="head_tilt_link"/>
    <origin xyz="0.05 0 0.05" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.76" upper="1.45" effort="10" velocity="1.57"/>
    <dynamics damping="0.5"/>
  </joint>

  <!-- RGBD Camera -->
  <link name="head_camera_link">
    <visual>
      <geometry>
        <box size="0.03 0.09 0.025"/>
      </geometry>
      <origin xyz="0 0 0"/>
      <material name="black"/>
    </visual>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="head_camera_joint" type="fixed">
    <parent link="head_tilt_link"/>
    <child link="head_camera_link"/>
    <origin xyz="0.08 0 0" rpy="0 0 0"/>
  </joint>

  <!-- Laser Scanner -->
  <link name="laser_link">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="laser_joint" type="fixed">
    <parent link="base_link"/>
    <child link="laser_link"/>
    <origin xyz="0.23 0 0.35" rpy="0 0 0"/>
  </joint>

</robot>
