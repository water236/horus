<?xml version="1.0"?>
<!-- Toyota HSR (Human Support Robot) - Simplified URDF for sim3d -->
<!-- Based on Toyota Research Institute HSR specifications -->
<robot name="hsr">

  <!-- Base Link (Omni-directional mobile base) -->
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder radius="0.22" length="0.24"/>
      </geometry>
      <origin xyz="0 0 0.12" rpy="0 0 0"/>
      <material name="hsr_white">
        <color rgba="0.9 0.9 0.9 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.22" length="0.24"/>
      </geometry>
      <origin xyz="0 0 0.12" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="30.0"/>
      <origin xyz="0 0 0.12"/>
      <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.5"/>
    </inertial>
  </link>

  <!-- Omni Wheel 1 -->
  <link name="omni_wheel_1_link">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.02"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/>
    </inertial>
  </link>

  <joint name="omni_wheel_1_joint" type="continuous">
    <parent link="base_link"/>
    <child link="omni_wheel_1_link"/>
    <origin xyz="0.15 0.13 0.04" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Omni Wheel 2 -->
  <link name="omni_wheel_2_link">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.02"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/>
    </inertial>
  </link>

  <joint name="omni_wheel_2_joint" type="continuous">
    <parent link="base_link"/>
    <child link="omni_wheel_2_link"/>
    <origin xyz="0.15 -0.13 0.04" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Omni Wheel 3 -->
  <link name="omni_wheel_3_link">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.02"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/>
    </inertial>
  </link>

  <joint name="omni_wheel_3_joint" type="continuous">
    <parent link="base_link"/>
    <child link="omni_wheel_3_link"/>
    <origin xyz="-0.15 0 0.04" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Torso (Telescoping column) -->
  <link name="torso_link">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.5"/>
      </geometry>
      <origin xyz="0 0 0.25" rpy="0 0 0"/>
      <material name="hsr_white"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.5"/>
      </geometry>
      <origin xyz="0 0 0.25" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 0 0.25"/>
      <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0" izz="0.05"/>
    </inertial>
  </link>

  <joint name="torso_lift_joint" type="prismatic">
    <parent link="base_link"/>
    <child link="torso_link"/>
    <origin xyz="-0.05 0 0.24" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="0.0" upper="0.35" effort="200" velocity="0.15"/>
    <dynamics damping="50"/>
  </joint>

  <!-- Shoulder -->
  <link name="arm_flex_link">
    <visual>
      <geometry>
        <box size="0.08 0.08 0.08"/>
      </geometry>
      <material name="hsr_white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.08 0.08 0.08"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.003" ixy="0" ixz="0" iyy="0.003" iyz="0" izz="0.003"/>
    </inertial>
  </link>

  <joint name="arm_flex_joint" type="revolute">
    <parent link="torso_link"/>
    <child link="arm_flex_link"/>
    <origin xyz="0 0.1 0.45" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.62" upper="0.0" effort="40" velocity="1.0"/>
    <dynamics damping="1.0"/>
  </joint>

  <!-- Arm Roll -->
  <link name="arm_roll_link">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.2"/>
      </geometry>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <material name="hsr_white"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.04" length="0.2"/>
      </geometry>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="1.2"/>
      <origin xyz="0 0 -0.1"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.002"/>
    </inertial>
  </link>

  <joint name="arm_roll_joint" type="revolute">
    <parent link="arm_flex_link"/>
    <child link="arm_roll_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-2.09" upper="3.84" effort="20" velocity="1.5"/>
    <dynamics damping="0.5"/>
  </joint>

  <!-- Wrist Flex -->
  <link name="wrist_flex_link">
    <visual>
      <geometry>
        <box size="0.06 0.06 0.06"/>
      </geometry>
      <material name="hsr_white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.06 0.06 0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.6"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="wrist_flex_joint" type="revolute">
    <parent link="arm_roll_link"/>
    <child link="wrist_flex_link"/>
    <origin xyz="0 0 -0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.92" upper="1.22" effort="10" velocity="2.0"/>
    <dynamics damping="0.5"/>
  </joint>

  <!-- Wrist Roll -->
  <link name="wrist_roll_link">
    <visual>
      <geometry>
        <cylinder radius="0.03" length="0.06"/>
      </geometry>
      <origin xyz="0 0 -0.03" rpy="0 0 0"/>
      <material name="hsr_white"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.03" length="0.06"/>
      </geometry>
      <origin xyz="0 0 -0.03" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.3"/>
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/>
    </inertial>
  </link>

  <joint name="wrist_roll_joint" type="revolute">
    <parent link="wrist_flex_link"/>
    <child link="wrist_roll_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.92" upper="3.67" effort="5" velocity="2.5"/>
    <dynamics damping="0.3"/>
  </joint>

  <!-- Hand -->
  <link name="hand_link">
    <visual>
      <geometry>
        <box size="0.08 0.06 0.02"/>
      </geometry>
      <material name="hsr_white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.08 0.06 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.3"/>
      <inertia ixx="0.0003" ixy="0" ixz="0" iyy="0.0003" iyz="0" izz="0.0003"/>
    </inertial>
  </link>

  <joint name="hand_joint" type="fixed">
    <parent link="wrist_roll_link"/>
    <child link="hand_link"/>
    <origin xyz="0 0 -0.07" rpy="0 0 0"/>
  </joint>

  <!-- Left Finger -->
  <link name="hand_l_finger_link">
    <visual>
      <geometry>
        <box size="0.06 0.01 0.04"/>
      </geometry>
      <material name="hsr_gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.06 0.01 0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.05"/>
      <inertia ixx="0.00005" ixy="0" ixz="0" iyy="0.00005" iyz="0" izz="0.00005"/>
    </inertial>
  </link>

  <joint name="hand_l_finger_joint" type="prismatic">
    <parent link="hand_link"/>
    <child link="hand_l_finger_link"/>
    <origin xyz="0.04 0.02 -0.03" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0.0" upper="0.04" effort="20" velocity="0.05"/>
  </joint>

  <!-- Right Finger -->
  <link name="hand_r_finger_link">
    <visual>
      <geometry>
        <box size="0.06 0.01 0.04"/>
      </geometry>
      <material name="hsr_gray"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.06 0.01 0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.05"/>
      <inertia ixx="0.00005" ixy="0" ixz="0" iyy="0.00005" iyz="0" izz="0.00005"/>
    </inertial>
  </link>

  <joint name="hand_r_finger_joint" type="prismatic">
    <parent link="hand_link"/>
    <child link="hand_r_finger_link"/>
    <origin xyz="0.04 -0.02 -0.03" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit lower="0.0" upper="0.04" effort="20" velocity="0.05"/>
    <mimic joint="hand_l_finger_joint"/>
  </joint>

  <!-- Head Pan -->
  <link name="head_pan_link">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.05"/>
      </geometry>
      <material name="hsr_white"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="head_pan_joint" type="revolute">
    <parent link="torso_link"/>
    <child link="head_pan_link"/>
    <origin xyz="0 0 0.55" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.84" upper="1.75" effort="5" velocity="2.0"/>
    <dynamics damping="0.3"/>
  </joint>

  <!-- Head Tilt -->
  <link name="head_tilt_link">
    <visual>
      <geometry>
        <box size="0.12 0.08 0.1"/>
      </geometry>
      <origin xyz="0.04 0 0" rpy="0 0 0"/>
      <material name="hsr_white"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.12 0.08 0.1"/>
      </geometry>
      <origin xyz="0.04 0 0" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.8"/>
      <origin xyz="0.04 0 0"/>
      <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
    </inertial>
  </link>

  <joint name="head_tilt_joint" type="revolute">
    <parent link="head_pan_link"/>
    <child link="head_tilt_link"/>
    <origin xyz="0 0 0.04" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-0.92" upper="0.79" effort="5" velocity="2.0"/>
    <dynamics damping="0.3"/>
  </joint>

  <!-- Head RGBD Camera -->
  <link name="head_rgbd_sensor_link">
    <visual>
      <geometry>
        <box size="0.025 0.09 0.025"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="head_rgbd_sensor_joint" type="fixed">
    <parent link="head_tilt_link"/>
    <child link="head_rgbd_sensor_link"/>
    <origin xyz="0.1 0 0" rpy="0 0 0"/>
  </joint>

  <!-- Base Laser Scanner -->
  <link name="base_laser_link">
    <visual>
      <geometry>
        <cylinder radius="0.03" length="0.04"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <mass value="0.15"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="base_laser_joint" type="fixed">
    <parent link="base_link"/>
    <child link="base_laser_link"/>
    <origin xyz="0.18 0 0.26" rpy="0 0 0"/>
  </joint>

  <!-- Hand Camera -->
  <link name="hand_camera_link">
    <visual>
      <geometry>
        <box size="0.02 0.03 0.02"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <mass value="0.02"/>
      <inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00001"/>
    </inertial>
  </link>

  <joint name="hand_camera_joint" type="fixed">
    <parent link="hand_link"/>
    <child link="hand_camera_link"/>
    <origin xyz="0 0 0.02" rpy="0 0.5 0"/>
  </joint>

</robot>
